# Tomplate with environment placeholders
# Tests: Section 5.5.2 - Environment Placeholders

server:
  host: [[HOST:-localhost]]
  port: [[PORT:-8080]]
  
database:
  url: [[DATABASE_URL:-postgres://localhost:5432/dev]]
  user: [[DB_USER]]
  password: [[DB_PASSWORD]]
  
aws:
  region: [[AWS_REGION:-us-east-1]]
  account: [[AWS_ACCOUNT_ID]]
  
# Multiple on same line
connection: [[DB_USER:-admin]]@[[HOST:-localhost]]:[[PORT:-5432]]
